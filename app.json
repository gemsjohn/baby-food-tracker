{
  "expo": {
    "name": "Baby Food Tracker",
    "description" : "Optimize your baby's nutrition & track progress. With our app, you can easily track your baby's feeding history, see nutritional values for every meal, and quickly add or remove entries with just a few taps.",
    "slug": "babyFoodTracker",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/favicon_0.png",
      "resizeMode": "contain",
      "backgroundColor": "#f7ff6c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.babyFoodTracker",
      "versionCode": 21,
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#f7ff6c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b9cdea52-7519-4f2d-a5e8-ceb28b5fb3bc"
      }
    }
  }
}
